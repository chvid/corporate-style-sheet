{
  "name": "corporate-style-sheet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prettier": "prettier --write src package.json",
    "preview": "vite preview"
  },
  "prettier": {
    "printWidth": 160,
    "tabWidth": 2,
    "singleQuote": false,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
